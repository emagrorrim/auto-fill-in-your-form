{
  "manifest_version": 2,
  "name": "Form Auto Fill In",
  "description": "Auto fill in your form using your saved information",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "background": {
    "scripts": ["./src/background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Form auto fill in"
  }
}